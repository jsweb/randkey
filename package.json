{
  "name": "@jsweb/randkey",
  "version": "3.0.4",
  "description": "Simple JS module to generate random id/key/hash in various formats, including UUID v4",
  "main": "dist/main.js",
  "esnext": "dist/esnext.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsweb/randkey.git"
  },
  "bugs": {
    "url": "https://github.com/jsweb/randkey/issues"
  },
  "homepage": "https://randkey.jsweb.app",
  "author": "Alex Bruno <git.alexbr@outlook.com>",
  "keywords": [
    "random",
    "id",
    "key",
    "hash",
    "uuid",
    "js"
  ],
  "license": "MIT",
  "scripts": {
    "dist": "run-s dist:*",
    "dist:clean": "rimraf dist",
    "dist:check": "tsc --noEmit",
    "dist:lint": "tslint -p . -c tslint.json",
    "dist:build": "rollup -c",
    "dist:minify": "minify dist/main.js -d dist",
    "dist:banner": "concat src/author.js dist/main.js -o dist/main.js",
    "report": "mocha dist/test --reporter mochawesome --reporter-options reportTitle=@jsweb/randkey",
    "test": "npm run dist && npm run report"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "babel-minify": "^0.4.3",
    "concat": "^1.0.3",
    "mocha": "^5.2.0",
    "mochawesome": "^3.0.3",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.65.2",
    "rollup-plugin-typescript": "^1.0.0",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  }
}
