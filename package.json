{
  "name": "@jsweb/randkey",
  "version": "3.3.1",
  "description": "Simple JS module to generate random id/key/hash in various formats, including UUID v4",
  "main": "dist/umd.js",
  "esnext": "dist/module.js",
  "module": "dist/module.js",
  "types": "dist/module.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsweb/randkey.git"
  },
  "bugs": {
    "url": "https://github.com/jsweb/randkey/issues"
  },
  "homepage": "https://randkey.jsweb.app",
  "author": "Alex Bruno CÃ¡ceres <git.alexbr@outlook.com>",
  "keywords": [
    "random",
    "id",
    "key",
    "hash",
    "uuid",
    "js"
  ],
  "license": "MIT",
  "scripts": {
    "dist": "run-s dist:*",
    "dist:clean": "rimraf dist",
    "dist:test": "npm t",
    "dist:types": "tsc --removeComments --emitDeclarationOnly",
    "dist:build": "rollup -c",
    "test": "run-s test:*",
    "test:lint": "tslint -p . -c tslint.json",
    "test:clean": "rimraf mocha*",
    "test:unit": "mocha src/**/*.test.ts --require ts-node/register",
    "test:report": "npm run test:unit -- --reporter mochawesome --reporter-options reportTitle=@jsweb/randkey",
    "prepare": "npm run dist"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "mocha": "^5.2.0",
    "mochawesome": "^3.0.3",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.65.2",
    "rollup-plugin-esmin": "^0.1.3",
    "rollup-plugin-typescript": "^1.0.0",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1"
  }
}
