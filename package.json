{
  "name": "@jsweb/randkey",
  "version": "3.0.3",
  "description": "Simple JS module to generate random id/key/hash in various formats, including UUID",
  "main": "dist/main.js",
  "esnext": "src/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsweb/randkey.git"
  },
  "bugs": {
    "url": "https://github.com/jsweb/randkey/issues"
  },
  "homepage": "https://randkey.jsweb.app",
  "author": "Alex Bruno <git.alexbr@outlook.com>",
  "keywords": [
    "random",
    "id",
    "key",
    "hash",
    "uuid",
    "js"
  ],
  "license": "MIT",
  "scripts": {
    "dist": "run-s dist:*",
    "dist:build": "rollup -c",
    "dist:minify": "minify dist/main.js -d dist",
    "dist:banner": "concat src/author.js dist/main.js -o dist/main.js",
    "report": "mocha dist/test --reporter mochawesome --reporter-options reportTitle=@jsweb/randkey",
    "test": "npm run dist && mocha dist/test"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "babel-minify": "^0.4.3",
    "concat": "^1.0.3",
    "mocha": "^5.2.0",
    "mochawesome": "^3.0.3",
    "npm-run-all": "^4.1.3",
    "rollup": "^0.65.1",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.4.0"
  }
}
