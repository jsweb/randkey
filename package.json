{
  "name": "@jsweb/randkey",
  "version": "3.3.4",
  "description": "Simple JS module to generate random id/key/hash in various formats, including UUID v4",
  "scripts": {
    "test": "run-s test:*",
    "test:lint": "npm run lint",
    "test:clean": "shx rm -rf mocha*",
    "test:unit": "mocha src/**/*.test.ts --require ts-node/register --reporter mochawesome --reporter-options reportTitle=@jsweb/randkey",
    "major": "npm version major && npm run push",
    "minor": "npm version minor && npm run push",
    "patch": "npm version patch && npm run push",
    "prepare": "npm run dist",
    "push": "git push && git push --tags",
    "lint": "tsc --noEmit && tslint -p . -c tslint.json --fix",
    "dist": "run-s dist:*",
    "dist:test": "npm t",
    "dist:clean": "shx rm -rf dist",
    "dist:build": "rollup -c",
    "dist:types": "tsc --removeComments --emitDeclarationOnly"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsweb/randkey.git"
  },
  "bugs": {
    "url": "https://github.com/jsweb/randkey/issues"
  },
  "homepage": "https://randkey.jsweb.app",
  "keywords": [
    "random",
    "id",
    "key",
    "hash",
    "uuid",
    "js"
  ],
  "author": "Alex Bruno CÃ¡ceres <git.alexbr@outlook.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "mocha": "^5.2.0",
    "mochawesome": "^3.0.3",
    "husky": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.0.0",
    "rollup-plugin-esmin": "^0.1.3",
    "rollup-plugin-typescript": "^1.0.0",
    "shx": "^0.3.2",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.3",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && git add ."
    }
  },
  "main": "dist/umd.js",
  "esnext": "dist/module.js",
  "module": "dist/module.js",
  "types": "dist/module.d.ts"
}
